# ─────────────────────────────────────────────────
# Prep Tool — Environment Variables
# Copy this file to .env and fill in your values.
# ─────────────────────────────────────────────────

# Flask secret key (auto-generated if unset)
# PREP_TOOL_SECRET=change-me-to-a-random-string

# Authentication (leave blank to disable login)
PREP_TOOL_USER=
PREP_TOOL_PASS=

# Server port
PREP_TOOL_PORT=5050

# Path to your logo image (optional)
# PREP_TOOL_LOGO=

# Flask debug mode (set to 0 in production)
FLASK_DEBUG=1
